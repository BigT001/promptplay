{
  "version": "0.1",
  "debug": {
    "enabled": true,
    "verbose": true,
    "trace": {
      "enabled": true,
      "level": "debug"
    },
    "logging": {
      "level": "DEBUG"
    }
  },
  "server": {
    "host": "0.0.0.0",
    "port": 8000,
    "workers": 1,
    "reload": true
  },
  "apps": {
    "script_writing_agent": {
      "type": "python",
      "path": "./script_writing_agent",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "debug": {
        "enabled": true,
        "trace": true,
        "verbose": true
      }
    }
  }
}
